<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.edexworldpc.ertsys.Profile"
    tools:showIn="@layout/app_bar_profile"
    android:orientation="vertical">


        <fragment
            android:name="com.example.edexworldpc.ertsys.Fragments.ProfilePicFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <fragment
            android:layout_marginTop="5dp"
            android:name="com.example.edexworldpc.ertsys.Fragments.ProfileItemListFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

</LinearLayout>
